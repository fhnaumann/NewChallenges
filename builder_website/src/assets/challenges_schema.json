            "type": "object"
        },
        "ChallengeMetadata": {
            "additionalProperties": false,
            "properties": {
                "builderMCVersion": {
                    "description": "MC Version the challenge builder targets at the time of creation.",
                    "type": "string"
                },
                "builderVersion": {
                    "description": "Version of the challenge builder used at the time of creation.",
                    "type": "string"
                },
                "createdBy": {
                    "default": "-",
                    "description": "Author of the challenge. The user who configured and built the challenge.",
                    "type": "string"
                },
                "lastModified": {
                    "description": "When the challenge was last modified. Is always set by the challenge builder.",
                    "format": "date-time",
                    "type": "string"
                },
                "name": {
                    "description": "Name of the challenge. Can be set by the user. Used to differentiate between challenges and to load a specific challenge.",
                    "type": "string"
                },
                "pluginMCVersion": {
                    "description": "MC Version the minecraft plugin targets at the time of loading the first time.",
                    "type": "string"
                },
                "pluginVersion": {
                    "description": "Version of the minecraft plugin used at the time of loading the first time.",
                    "type": "string"
                },
                "whenCreated": {
                    "description": "When the challenge was first initially created. Is always set by the challenge builder.",
                    "format": "date-time",
                    "type": "string"
                }
            },
            "propertyOrder": [
                "name",
                "whenCreated",
                "lastModified",
                "createdBy",
                "builderVersion",
                "pluginVersion",
                "builderMCVersion",
                "pluginMCVersion"
            ],
            "required": [
                "builderMCVersion",
                "builderVersion",
                "createdBy",
                "name",
                "pluginMCVersion",
                "pluginVersion"
            ],
            "type": "object"
        },
        "CollectableDataConfig": {
            "additionalProperties": false,
            "properties": {
                "amountNeeded": {
