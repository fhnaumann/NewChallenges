                "whenCollectedSeconds": {
                    "default": -1,
                    "description": "The time in seconds (since the start) that it took to complete this collectable. -1 indicates that is has not been collected (e.g. 'complete' is false).",
                    "type": "integer"
                }
            },
            "propertyOrder": [
                "whenCollectedSeconds",
                "contributors"
            ],
            "required": [
                "whenCollectedSeconds"
            ],
            "type": "object"
        },
        "ContributorsConfig": {
            "additionalProperties": {
                "type": "integer"
            },
            "propertyOrder": [],
            "type": "object"
        },
        "CraftingGoalConfig": {
